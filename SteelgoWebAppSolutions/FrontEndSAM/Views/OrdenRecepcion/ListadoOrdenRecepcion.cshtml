    CargaInicial();


    }
    @section JavascriptDocumentReadyHomeCookie {
    $authorizationModel["Orden Recepcion"] = $ListadoOrdenRecepcionModel;
    Cookies.set("home", true, { path: '/' });
    Cookies.set("navegacion", "15", { path: '/' });
    }

</script>
