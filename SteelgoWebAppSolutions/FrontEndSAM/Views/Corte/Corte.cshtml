    function VentanaModal(id) {

        var modalTitle = "";

        switch (id) {
            case 1:
                $("#modalSobrante").hide();
                $("#modalMermaPrimerIntento").hide();
                $("#modalMermaSegundoIntento").hide();
                $("#modalTolerancia").show();
                modalTitle = "Tolerancia";
                break;
            case 2:
                $("#modalMermaPrimerIntento").hide();
                $("#modalMermaSegundoIntento").hide();
                $("#modalTolerancia").hide();
                $("#modalSobrante").show();
                modalTitle = "Sobrante";
                break;
            case 3:
                $("#modalSobrante").hide();
                $("#modalMermaSegundoIntento").hide();
                $("#modalTolerancia").hide();
                $("#modalMermaPrimerIntento").show();
                modalTitle = "Merma";
                break;
            case 4:
                $("#modalSobrante").hide();
                $("#modalMermaPrimerIntento").hide();
                $("#modalTolerancia").hide();
                $("#modalMermaSegundoIntento").show();
                modalTitle = "Merma";
                break;
        };

        var window = $("#window");
        var win = window.kendoWindow({
            actions: false,
            modal: true,
            title: modalTitle,
            resizable: false,
            visible: false,
            width: "32.6%",
            minWidth: 660,
            position: {
                top: "10%",
                left: "20%"
            },
            open: onOpen,
            refresh: onRefresh
        }).data("kendoWindow");
        window.data("kendoWindow").title(modalTitle);
        window.data("kendoWindow").center().open();
    };

